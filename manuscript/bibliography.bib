  urldate = {2016-07-21},
  date = {2012-03-13},
  pages = {4187--4190},
  keywords = {biological time,haldanes,IARPA_Related,pedomorphosis,Scaling},
  file = {Full Text PDF:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\SREUQHSC\\Evans et al. - 2012 - The maximum rate of mammal evolution.pdf:application/pdf;Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\FP2WFCUV\\4187.html:text/html},
