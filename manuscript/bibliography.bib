@article{Zhang2017,
  title = {Modeling the Evolution of System Technology Performance When Component and System Technology Performances Interact: {{Commensalism}} and Amensalism},
  issn = {0040-1625},
  url = {http://www.sciencedirect.com/science/article/pii/S0040162517305140},
