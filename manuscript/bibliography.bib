  author = {Valverde, Sergi and Solé, Ricard V.},
  urldate = {2016-07-12},
  date = {2015-06-06},
  pages = {20150249},
  keywords = {IARPA_Related,Technology_Development},
  file = {Full Text PDF:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\K5GNDZN4\\Valverde and Solé - 2015 - Punctuated equilibrium in the large-scale evolutio.pdf:application/pdf;Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\ASSP3BSV\\20150249.html:text/html}
