  series = {Handbook of The Economics of Innovation, Vol. 1},
  publisher = {{North-Holland}},
  author = {Dosi, Giovanni and Nelson, Richard R.},
  editor = {Rosenberg, Bronwyn H. Hall and Nathan},
  urldate = {2016-07-27},
  date = {2010},
  pages = {51--127},
  keywords = {capability-based theories of the firm,emergent properties,endogenous growth,Evolution,IARPA_Related,industrial dynamics,innovation,Learning,selection,technological paradigms,technological regimes and trajectories},
  file = {ScienceDirect Full Text PDF:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\BDGCHZJU\\Dosi and Nelson - 2010 - Chapter 3 - Technical Change and Industrial Dynami.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\HSQSBFSC\\S0169721810010038.html:text/html}
