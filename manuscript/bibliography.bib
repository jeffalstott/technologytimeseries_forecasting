  journaltitle = {Journal of Statistical Physics},
  shortjournal = {J Stat Phys},
  author = {Johnson, Neil F. and Manrique, Pedro and Hui, Pak Ming},
  urldate = {2016-12-23},
  date = {2013-05-01},
  pages = {395--413},
  keywords = {_tablet,IARPA_Related},
