  keywords = {IARPA_Related,Lotka-Volterra equations,Technology amensalism,Technology commensalism,Technology ecosystem,Technology evolution,Technology prediction},
  file = {Zhang et al_2017_Modeling the evolution of system technology performance when component and.pdf:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\5AURVB85\\Zhang et al_2017_Modeling the evolution of system technology performance when component and.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\UHCA2S8H\\S0040162517305140.html:text/html}
