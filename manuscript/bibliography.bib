  pages = {1039--1074},
  keywords = {Asymptotic equipartition,Bayesian consistency,Bayesian nonparametrics,Egorov’s theorem,IARPA_Related,large deviations,posterior convergence,replicator dynamics,sofic systems},
  file = {Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\H35GFDGN\\1256822130.html:text/html},
