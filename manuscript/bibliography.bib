@incollection{Vitanov2016,
  title = {Science and {{Society}}. {{Assessment}} of {{Research}}},
  rights = {©2016 Springer International Publishing Switzerland},
  isbn = {978-3-319-41629-8 978-3-319-41631-1},
  url = {http://link.springer.com/chapter/10.1007/978-3-319-41631-1_1},
  abstract = {Science is a driving force of positive social evolution. And in the course of this evolution, research systems change as a consequence of their complex dynamics. Research systems must be managed very carefully, for they are dissipative, and their evolution takes place on the basis of a series of instabilities that may be constructive (i.e., can lead to states with an increasing level of organization) but may be also destructive (i.e., can lead to states with a decreasing level of organization and even to the destruction of corresponding systems). For a better understanding of relations between science and society, two selected topics are briefly discussed: the Triple Helix model of a knowledge-based economy and scientific competition among nations from the point of view of the academic diamond. The chapter continues with a part presenting the minimum of knowledge necessary for understanding the assessment of research activity and research organizations. This part begins with several remarks on the assessment of research and the role of research publications for that assessment. Next, quality and performance as well as measurement of quality and latent variables by sets of indicators are discussed. Research activity is a kind of social process, and because of this, some differences between statistical characteristics of processes in nature and in society are mentioned further in the text. The importance of the non-Gaussianity of many statistical characteristics of social processes is stressed, because non-Gaussianity is connected to important requirements for study of these processes such as the need for multifactor analysis or probabilistic modeling. There exist entire branches of science, scientometrics, bibliometrics, informetrics, and webometrics, which are devoted to the quantitative perspective of studies on science. The sets of quantities that are used in scientometrics are mentioned, and in addition, we stress the importance of understanding the inequality of scientific achievements and the usefulness of knowledge landscapes for understanding and evaluating research performance. Next, research production and its assessment are discussed in greater detail. Several examples for methods and systems for such assessment are presented. The chapter ends with a description of an example for a combination of qualitative and quantitative tools in the assessment of research: the English–Czerwon method for quantification of scientific performance.},
  timestamp = {2016-08-07T11:08:38Z},
  langid = {english},
  booktitle = {Science {{Dynamics}} and {{Research Production}}},
  series = {Qualitative and Quantitative Analysis of Scientific and Scholarly Communication},
  publisher = {{Springer International Publishing}},
  author = {Vitanov, Nikolay K.},
  urldate = {2016-08-07},
  date = {2016},
  pages = {3--52},
  keywords = {IARPA_Related,Methodology of the Social Sciences,Nonlinear Dynamics,Printing and Publishing,R & D/Technology Policy,Statistical Theory and Methods},
  file = {Full Text PDF:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\W36Q2U7F\\Vitanov - 2016 - Science and Society. Assessment of Research.pdf:application/pdf;Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\C4VHGDEP\\978-3-319-41631-1_1.html:text/html},
  doi = {10.1007/978-3-319-41631-1_1}
