@article{Basnet2016b,
  title = {Modeling of Technological Performance Trends Using Design Theory},
  volume = {2},
  issn = {2053-4701},
  url = {http://journals.cambridge.org/article_S2053470116000081},
  doi = {10.1017/dsj.2016.8},
  abstract = {Functional technical performance usually follows an exponential dependence on time but the rate of change (the exponent) varies greatly among technological domains. This paper presents a simple model that provides an explanatory foundation for these phenomena based upon the inventive design process. The model assumes that invention – novel and useful design – arises through probabilistic analogical transfers that combine existing knowledge by combining existing individual operational ideas to arrive at new individual operating ideas. The continuing production of individual operating ideas relies upon injection of new basic individual operating ideas that occurs through coupling of science and technology simulations. The individual operational ideas that result from this process are then modeled as being assimilated in components of artifacts characteristic of a technological domain. According to the model, two effects (differences in interactions among components for different domains and differences in scaling laws for different domains) account for the differences found in improvement rates among domains whereas the analogical transfer process is the source of the exponential behavior. The model is supported by a number of known empirical facts: further empirical research is suggested to independently assess further predictions made by the model.},
  timestamp = {2016-07-11T10:05:57Z},
  journaltitle = {Design Science},
  author = {Basnet, Subarna and Magee, Christopher L.},
  urldate = {2016-07-11},
  date = {2016},
