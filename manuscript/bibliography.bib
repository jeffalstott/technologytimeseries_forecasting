  shortjournal = {J Inf Technol},
  author = {Flath, Christoph M. and Friesike, Sascha and Wirth, Marco and Thiesse, Frédéric},
  urldate = {2017-08-20},
  date = {2017-08-15},
  pages = {1--20},
