@article{dAndria,
  title = {A {{Win}}-{{Win}}-{{Win}}? {{Motivating}} Innovation in a Knowledge Economy with Tax Incentives},
  issn = {0040-1625},
  url = {http://www.sciencedirect.com/science/article/pii/S0040162517307308},
  doi = {10.1016/j.techfore.2017.05.030},
  shorttitle = {A {{Win}}-{{Win}}-{{Win}}?},
  abstract = {In this study we explore effects of two distinct tax policies on innovation in a pure knowledge economy: an ‘IP box’ incentive and a (hypothetical) tax incentive on compensation earned by agents from profit sharing schemes (PSS). In contrast to the conventional assumption that firms decide on whether to innovate or not, we focus on a bottom-up innovation process (sometimes also called ‘bootleg innovation’), where firms set incentives to fulfill different tasks, but the final decision on whether to make the more innovative task is taken by an employee. We compare the two tax incentives under several distinct specifications demonstrating that the tax incentive on PSS can be a powerful mechanism fostering innovative activity and benefiting at the same time workers, firms and the economy as a whole. This study shows that the more critical for firms is attracting and motivating highly skilled workers, the larger the expected gain from employing the tax incentive on agents' compensation. We also find that the relative efficacy of this tax incentive is moderated by labor mobility and the extent of knowledge spillovers.},
  timestamp = {2017-07-01T23:01:27Z},
  journaltitle = {Technological Forecasting and Social Change},
  shortjournal = {Technological Forecasting and Social Change},
  author = {d’ Andria, D. and Savin, I.},
  keywords = {Bottom-up innovation,Financing of R&D investments,IARPA_Related,Knowledge economy,Profit sharing schemes,Tax incentives for R&D},
  options = {useprefix=true},
  file = {d’Andria_Savin_A Win-Win-Win.pdf:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\A9HJNMD3\\d’Andria_Savin_A Win-Win-Win.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\3D6KPF7W\\S0040162517307308.html:text/html}
