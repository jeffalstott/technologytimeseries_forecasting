  date = {2016-01-30},
  keywords = {IARPA_Related,Quantitative Finance - Economics,Quantitative Finance - General Finance},
  file = {arXiv\:1602.00090 PDF:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\9EK7E9G2\\Magee and Devezas - 2016 - A Simple extension of Dematerialization Theory In.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\Z5EHZRPB\\1602.html:text/html}
