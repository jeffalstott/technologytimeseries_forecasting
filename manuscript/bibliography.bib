  file = {Johnson et al_2016_New online ecology of adversarial aggregates.pdf:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\7SE5HJK4\\Johnson et al_2016_New online ecology of adversarial aggregates.pdf:application/pdf;Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\VKS9HFX2\\1459.html:text/html},
