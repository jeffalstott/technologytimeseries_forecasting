  langid = {english},
  number = {6154},
  journaltitle = {Science},
  author = {Wang, Dashun and Song, Chaoming and Barabási, Albert-László},
  urldate = {2016-07-25},
  date = {2013-10-04},
  pages = {127--132},
  keywords = {IARPA_Related},
  file = {Full Text PDF:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\JV2H89HC\\Wang et al. - 2013 - Quantifying Long-Term Scientific Impact.pdf:application/pdf;Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\7X26PQMB\\127.html:text/html},
