  date = {2015-02-01},
  pages = {191--201},
  keywords = {_tablet,IARPA_Related},
  file = {MacKay_2015_When Lanchester met Richardson, the outcome was stalemate.pdf:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\RWVZD64T\\MacKay_2015_When Lanchester met Richardson, the outcome was stalemate.pdf:application/pdf;Snapshot:C\:\\Users\\jeff\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\dlun4vfq.default\\zotero\\storage\\PE25AX53\\jors.2013.html:text/html}
}

@article{Schutte2011,
